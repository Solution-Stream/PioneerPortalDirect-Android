<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pioneer.portaldirect"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pioneer.portaldirect.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteListActivity"
            android:label="@string/title_activity_quote_list" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteTabActivity"
            android:label="@string/title_activity_quote_tab" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteApplicantActivity"
            android:label="@string/title_activity_quote_applicant" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteDriverActivity"
            android:label="@string/title_activity_quote_driver" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteVehicleActivity"
            android:label="@string/title_activity_quote_vehicle" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteCoverageActivity"
            android:label="@string/title_activity_quote_coverage" >
        </activity>
        <activity
            android:name="com.pioneer.portaldirect.QuoteReviewActivity"
            android:label="@string/title_activity_quote_review" >
        </activity>
    </application>

</manifest>
